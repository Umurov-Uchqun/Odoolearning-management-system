<odoo>
    <data>
        <record model="ir.ui.view" id="le_schedule_table_list">
            <field name="name">schedule table list</field>
            <field name="model">le.schedule.table</field>
            <field name="arch" type="xml">
                <list>
                    <field name="name"/>
                </list>
            </field>
        </record>

        <record model="ir.ui.view" id="le_schedule_table_form">
            <field name="name">schedule table form</field>
            <field name="model">le.schedule.table</field>
            <field name="arch" type="xml">
                <form>
                    <field name="name"/>
                </form>
            </field>
        </record>

        <record id="le_schedule_table_action" model="ir.actions.act_window">
            <field name="name">schedule table</field>
            <field name="res_model">le.schedule.table</field>
            <field name="view_mode">list,form</field>
        </record>

        <menuitem id="schedule_table_menu" action="le_schedule_table_action"/>
    </data>
</odoo>